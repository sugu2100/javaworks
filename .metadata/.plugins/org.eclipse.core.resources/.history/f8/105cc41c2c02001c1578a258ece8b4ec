package forfor;

import java.util.Scanner;

public class Seats {

	public static void main(String[] args) {
		// ÁÂ¼® ¹èÄ¡µµ
		/*int i, j;
		for(i=0; i<5; i++) {
			for(j=1; j<=5; j++) {
				int inc = i*5+j;
				System.out.print("ÁÂ¼®" + inc + " ");
			}
			System.out.println();
		}*/
		
		Scanner sc = new Scanner(System.in);
		int i, j;
		
		System.out.print("ÀÔÀå°´ ¼ö ÀÔ·Â : ");
		int customNum = sc.nextInt();
		
		System.out.print("ÁÂ¼® ¿­ ¼ö ÀÔ·Â : ");
		int colNum = sc.nextInt();
		
		int rowNum;
		if(customNum % colNum == 0) {
			rowNum = customNum / colNum;
		}else {
			rowNum = customNum / colNum + 1;
		}
		
		for(i=0; i<rowNum; i++) {
			for(j=0; j<=colNum; j++) {
				int seats = i*colNum+j;
				System.out.print("ÁÂ¼®" + seats + " ");
				if(seats == customNum)
					break;
			}
		}
		
		sc.close();
	}
}
