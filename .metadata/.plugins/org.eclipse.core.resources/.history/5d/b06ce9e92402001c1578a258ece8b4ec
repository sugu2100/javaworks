package forfor;

public class StarPrint {

	public static void main(String[] args) {
		// º° Âï±â
		for(int i=1; i<=5; i++) {
			for(int j=1; j<=5; j++) {
				System.out.print("*");
			}
			System.out.println(); //ÁÙ¹Ù²Þ
		}
		
		//»ï°¢Çü ¸ð¾ç º° Âï±â
		/*
		 *
		 **
		 ***
		 ****
		 *****
		 */
		
		for(int i=1; i<=5; i++) {
			for(int j=1; j<=i; j++) {
				System.out.print("*");
			}
			System.out.println(); //ÁÙ¹Ù²Þ
		}
		
		
		
		
		
		
		
		
		
		
	}
}
