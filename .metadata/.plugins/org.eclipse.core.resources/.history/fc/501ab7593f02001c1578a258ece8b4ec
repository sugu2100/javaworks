package arrays;

public class FindMax {

	public static void main(String[] args) {
		// 최대값 찾기
		int[] arr = {3, 7, 2, 5, 8, 4};
		int max = arr[0];  //max값을 0번 인덱스로 초기화
		
		for(int i=1; i<arr.length; i++) {
			if(max < arr[i]) { //최대값과 비교
				max = arr[i];  //최대값을 변경
			}
		}
		System.out.println("max=" + max);

	}

}
